<script language="javascript">
function printName(){
  var result = './SpatialFeaturePlot/' + document.getElementById('name').value + '.jpeg';
  document.getElementById("imgId").src = result

  var result2 = './VlnPlot/' + document.getElementById('name').value + '.jpeg';
  document.getElementById("imgId2").src = result2
}
</script>

<form>
<div>
<label for="uname">Choose a genename: </label>
<input type = "text" id="name" name="name" onkeyup='printName()' required size="45" />
<span class="validity"></span>
</div>
</form>
<img id="imgId" src="">
<br/>
<img id="imgId2" src="">

</body>
</html>
