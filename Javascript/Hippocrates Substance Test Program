<script> 
  function dual1(char, obj){
    if(obj.style.background == 'white'){
      obj.style.background = 'yellow';
      document.getElementById('result').value ++;
      if(char == '1'){
        document.getElementById('resulta').value ++;
      }
      if(char == '2'){
        document.getElementById('resultb').value ++;
      }
      if(char == '3'){
        document.getElementById('resultc').value ++;
      }
      if(char == '4'){
        document.getElementById('resultd').value ++;
      }
    }  else if(obj.style.background == 'yellow'){
      obj.style.background = 'white';
      document.getElementById('resultt').value --;
      if(char == '1'){  
        document.getElementById('resulta').value --;
      }
      if(char == '2'){
        document.getElementById('resulta').value --;
      }
      if(char == '3'){
        document.getElementById('resulta').value --;   
      }
      if(char == '4'){
        document.getElementById('resulta').value --;   
      }
    } else{
      obj.style.background = 'white';          
    }
  }
</script>
